    <script>

    var modal = document.getElementById("modalBox");
    var uploadBox = document.getElementById("uploadBox");
    var close = document.getElementsByClassName("closebtn")[0];
    var span = document.getElementsByClassName("close")[0];
    var span2 = document.getElementsByClassName("close2")[0];
    var backbtn = document.getElementById("backbtn");


    $('.show').click(function() {
    id = $(this).attr('id');
    modal.style.display = "block";
    var source = document.getElementsByClassName(id)[0].src;
    document.getElementById("popImage").src =source;
    });

    $('.fill').click(function() {
     confirm("Are you sure you want to delete this medical file?");
    });

    $('.upload').click(function() {
     uploadBox.style.display = "block";
    });

    backbtn.onclick = function() {
      uploadBox.style.display = "none";
    }

    span.onclick = function() {
      modal.style.display = "none";
    }
    span2.onclick = function() {
      uploadBox.style.display = "none";
    }
    close.onclick = function() {
      modal.style.display = "none";
    }

    // When the user clicks anywhere outside of the modal, close it
    window.onclick = function(event) {
      if (event.target == modal) {
        modal.style.display = "none";
      }
    }
    </script>
