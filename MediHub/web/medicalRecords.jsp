    <div id="uploadBox" class="modal">
      <div class="modal-content">
        <span class="close2">&times;</span>
        <h2>Upload Medical Record</h2>
        <hr>
        <form>
          <input type="file" name="mRecord">

          <div class="record">
            <div class="label2"> Name </div>
            <input class="data2" type="text" name="name" id="name" placeholder=" Name for Medical Record">
          </div>

          <div class="record">
            <div class="label2">Record Catergory</div>
            <select class="data2" name="category_id" id="category_id"  style="height: 50px;">
              <option disabled="disabled" selected="selected">--Choose Option--</option>
              <option>Blood Report</option>
              <option>ECG Report</option>
              <option>X-ray Report</option>
              <option>Other</option>
            </select>
          </div>

          <div class="record">
            <div class="label2">Hospital Name </div>
            <input class="data2" type="text" name="hospital_name" id="hospital_name" placeholder=" Name of Hospital">
          </div>

          <div class="record">
            <div class="label2">Doctor Name </div>
            <input class="data2" type="text" name="doctor_name" id="doctor_name" placeholder=" Name of Doctor">
          </div>

          <div class="record">
            <div class="label2">Date </div>
            <input class="data2" type="text" name="date" id="date" placeholder="Issued date of medical record">
          </div>

          <div class="record">
            <div class="label2">Time </div>
            <input class="data2" type="text" name="time" id="time" placeholder="Issued time of medical record">
          </div>
        </form>
        <button class="styled"  id="backbtn"type="button" style="margin-top: 30px;width:25%; line-height:50px;"> Back </button>
        <button class="styled" type="button" style="margin-top: 30px;width:25%;line-height:50px;"> Upload </button>
        <center>
      </div>

